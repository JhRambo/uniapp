
.wx-nav {
	text-align: center;
	width: 100%;
	height: 200rpx;
	line-height: 200rpx;
	display: flex;
	justify-content: space-between;
	align-items: center;
	font-size: 32rpx;
	color: #636263;
}
.icons {
	font-size: 50rpx;
	padding: 10rpx;
}
.scroll-content {
	width: 100%;
	height: 80rpx;
	white-space: nowrap;
}
.scroll-item {
	display: inline-block;
	padding: 10rpx 20rpx;
}
.f-active-color {
	text-decoration: underline;
}

